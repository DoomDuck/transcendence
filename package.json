{
	"name": "transcendence",
	"version": "0.0.1",
	"description": "42's Common core end project",
	"directories": {
		"doc": "doc"
	},
	"scripts": {
		"front_build": "webpack",
		"front_debug": "webpack -d source-map",
		"front_build:dev": "webpack --watch",
		"nest_build": "nest build --path tsconfig.back.json",
		"nest_start": "nest start --path tsconfig.back.json",
		"nest_start:dev": "nest start --path tsconfig.back.json --watch",
		"nest_start:debug": "nest start --path tsconfig.back.json --debug --watch",
		"nest_start:prod": "node build/back/main.js",
		"check_format": "./scripts/check_format.sh",
		"format": "./scripts/format.sh",
		"setup_hooks": "./scripts/hooks/setup.sh",
		"remove_hooks": "./scripts/hooks/remove.sh"
	},
	"repository": {
